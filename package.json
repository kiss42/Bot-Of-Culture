{
  "name": "bot-of-culture",
  "version": "1.0.0",
  "description": "A Discord Bot curated for the Men of Culture discord server",
  "main": "app.js",
  "repository": "git@github.com:4upz/Bot-Of-Culture.git",
  "author": "Arik Smith (4upz) <ams382@case.edu>",
  "license": "MIT",
  "engines": {
    "node": ">=16.x"
  },
  "private": "true",
  "scripts": {
    "start": "node ./dist/app.js",
    "dev": "ts-node-dev src/app.ts",
    "dev:tutorial": "nodemon src/tutorial/legacyApp.js",
    "build": "tsc -p .",
    "prisma:generate": "prisma generate",
    "deploy-commands": "ts-node scripts/deployCommands.ts",
    "deploy-commands:global": "ts-node scripts/deployCommands.ts global",
    "deploy:prod": "gcloud app deploy app.yml"
  },
  "devDependencies": {
    "@types/needle": "^2.5.3",
    "@types/node": "^18.11.3",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "nodemon": "^2.0.20",
    "prettier": "2.7.1",
    "prisma": "^4.5.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@prisma/client": "^4.5.0",
    "dayjs": "^1.11.6",
    "discord-interactions": "^3.2.0",
    "discord.js": "^14.6.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "needle": "^3.1.0",
    "node-fetch": "^3.2.10"
  },
  "keywords": [
    "discord",
    "node"
  ]
}
